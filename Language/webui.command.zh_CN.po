msgctxt "O1"
msgid "Command line help:"
msgstr "命令行帮助："

msgctxt "O2"
msgid "Display command line help information."
msgstr "显示命令行帮助信息"

msgctxt "O3"
msgid "Set the program language."
msgstr "设置程序语言"

msgctxt "O4"
msgid "The IP address you want to bind."
msgstr "你想关联的IP地址。"

msgctxt "O5"
msgid "The port of IP address you want to bind."
msgstr "你想关联的IP地址的端口。"

msgctxt "O6"
msgid "The path to SSL certificate."
msgstr "SSL证书路径。"

msgctxt "O7"
msgid "The path to SSL private key."
msgstr "SSL私钥路径。"

msgctxt "O8"
msgid "You need to give both the SSL certificate and the SSL private key."
msgstr "你需要同时给出SSL证书和SSL私钥。"

msgctxt "O9"
msgid "The path to SSL certificate chain."
msgstr "SSL证书链路径。"

msgctxt "O10"
msgid "Enable password verification and set the password."
msgstr "启用密码验证并设置密码。"

msgctxt "O11"
msgid "The length of the password needs to be between <min> and <max>."
msgstr "密码的长度必须在<min>-<max>之间。"
