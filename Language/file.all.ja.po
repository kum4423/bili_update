msgctxt "OUTPUT1"
msgid "NO.\tFile name\t"
msgstr "いいえ\tファイル名\t"

msgctxt "OUTPUT2"
msgid "Type\tLast access time\tCreate time\t\tLast modify time\tFile size"
msgstr "タイプ\t最終アクセス時間\t作成時間\t\t最終更新時間\tファイルサイズ"

msgctxt "DIR"
msgid "DIR"
msgstr "ディレクトリ"

msgctxt "FILE"
msgid "FILE"
msgstr "ファイル"

msgctxt "OUTPUT3"
msgid "Page <num> of <count>"
msgstr "ページ <count> の <num>"

msgctxt "O1"
msgid "Previous page"
msgstr "前のページ"

msgctxt "O2"
msgid "Next page"
msgstr "次のページ"

msgctxt "O3"
msgid "Parent folder"
msgstr "上の階層"

msgctxt "O4"
msgid "Do not display files with no file extensions"
msgstr "拡張子のないファイルを表示しない"

msgctxt "O5"
msgid "Display files with no file extensions"
msgstr "拡張子のないファイルを表示する"

msgctxt "O6"
msgid "Display all files"
msgstr "全てのファイルを表示する"

msgctxt "O7"
msgid "Display files which pass the filter"
msgstr "フィルタを通過したファイルを表示する"

msgctxt "O8"
msgid "Enter folder name"
msgstr "フォルダ名を入力する"

msgctxt "O9"
msgid "Enter file name"
msgstr "ファイル名を入力する"

msgctxt "OUTPUT4"
msgid ""
"Please enter the number before file/directory name to select/enter the "
"file/directory or the letter to do that operation:"
msgstr "ファイル/ディレクトリ名の前に番号を入力して、ファイル/ディレクトリを選択/入力するか、その操作を行う文字を入力してください。"

msgctxt "ERROR1"
msgid "There is no parent folder."
msgstr "上の階層がありません。"

msgctxt "INPUT1"
msgid ""
"Please enter the location of folder (Support absolute and relative "
"location):"
msgstr "フォルダの場所を入力してください（絶対位置と相対位置をサポートしています）:"

msgctxt "ERROR2"
msgid "This folder does not exist."
msgstr "このフォルダは存在しません。"

msgctxt "INPUT2"
msgid "Please enter the file name:"
msgstr "ファイル名を入力してください:"

msgctxt "ERROR3"
msgid "This file has been selected before."
msgstr "このファイルは以前にも選択されています。"

msgctxt "INPUT3"
msgid "This file already exists, do you want to overwrite it?"
msgstr "このファイルは既に存在しますが、上書きしますか？"

msgctxt "INPUT4"
msgid "Do you want to select the file \"<filename>\"?"
msgstr "ファイル \"<filename>\" を選択しますか?"
