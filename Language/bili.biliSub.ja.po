msgctxt "INPUT1"
msgid "The file \"<filename>\" already exists, do you want to overwrite it?"
msgstr "ファイル \"<filename>\" は既に存在していますが、上書きしますか？"

msgctxt "OUTPUT1"
msgid "Failed to delete the original file, skip download."
msgstr "元のファイルの削除に失敗し、ダウンロードをスキップしました。"

msgctxt "OUTPUT2"
msgid "<languagename> subtitles for part <number> download complete."
msgstr "パート <number> の <languagename> 字幕のダウンロードが完了しました。"

msgctxt "ERROR1"
msgid "Failed to save \"<filename>\"."
msgstr "\"<filename>\" の保存に失敗しました。"

msgctxt "ERROR2"
msgid "Failed to write to the file \"<filename>\"."
msgstr "ファイル \"<filename>\"  への書き込みに失敗しました。"

msgctxt "OUTPUT3"
msgid "<languagename> lyrics for part <number> download complete."
msgstr "パート <number> の <languagename> 歌詞のダウンロードが完了しました。"

msgctxt "AULRCCOM"
msgid "<languagename> lyric download completed."
msgstr "<languagename> 歌詞のダウンロードが完了しました。"
