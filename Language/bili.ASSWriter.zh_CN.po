msgctxt "COM_MUST_STR"
msgid "The comment must be a string."
msgstr "评论必须是字符串。"

msgctxt "COLOR_MUST_INT"
msgid "The color value must be a integer."
msgstr "颜色的值必须是整数。"

msgctxt "COLOR_OUT_OF_RANGE"
msgid "The color value must between 0 and 255."
msgstr "颜色的值必须在0-255之间。"

msgctxt "INVALID_ASS_COLOR"
msgid "\"<value>\" is a invalid color in ASS script."
msgstr "\"<value>\"在ASS脚本中是无效的颜色。"

msgctxt "INVALID_CSS_COLOR"
msgid "\"<value>\" is a invalid color."
msgstr "\"<value>\"是无效的颜色。"

msgctxt "OUT_OF_RANGE"
msgid "This value must be greater than 0 or equal to 0."
msgstr "该值必须大于等于0。"

msgctxt "MUST_NUM"
msgid "This value must be a number."
msgstr "该值必须是数字。"

msgctxt "MUST_BOOL"
msgid "This value must be a boolean."
msgstr "该值必须是布尔型。"

msgctxt "ALIGN_OUT_OF_RANGE"
msgid "This value must between 1 and 9."
msgstr "该值必须在1-9之间。"
