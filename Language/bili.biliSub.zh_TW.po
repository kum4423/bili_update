msgstr "刪除原有文件失敗，跳過下載。"

msgctxt "OUTPUT2"
msgid "<languagename> subtitles for part <number> download complete."
msgstr "第<number>P<languagename>字幕下載完畢！"

msgctxt "ERROR1"
msgid "Failed to save \"<filename>\"."
msgstr "儲存\"<filename>\"失敗！"

msgctxt "ERROR2"
msgid "Failed to write to the file \"<filename>\"."
msgstr "寫入到文件\"<filename>\"時失敗！"

msgctxt "OUTPUT3"
msgid "<languagename> lyrics for part <number> download complete."
msgstr "第<number>P<languagename>歌詞下載完畢！"

