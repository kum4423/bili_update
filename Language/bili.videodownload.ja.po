msgid "Smooth quality"
msgstr "清画質"

msgctxt "AU标准"
msgid "Standard quality"
msgstr "標準画質"

msgctxt "AU高品质"
msgid "High quality"
msgstr "高画質"

msgctxt "AU无损音质"
msgid "Lossless quality"
msgstr "オリジナル画質"

